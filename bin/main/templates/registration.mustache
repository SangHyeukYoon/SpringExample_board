{{>layout/header}}

<h1>회원가입</h1>

<div class="col-md-12">
    <div class="col-md-4">

        <form method="POST">
            <div class="form-group">
            <label for="username">아이디</label>
            <input type="text" class="form-controll" id="username" placeholder="아이디">
            </div>

            <div class="form-group">
            <label for="password">비밀번호</label>
            <input type="password" class="form-controll" id="password" placeholder="비밀번호">
            </div>

            <a href="/" role="button" class="btn btn-secondary">취소</a>
            <button type="button" class="btn btn-primary" id="btn-register">가입</button>
        </form>

        

    </div>
</div>

{{>layout/footer}}